{
    "image": "kfarrow/dax:1.0",
    "features": [
        "workdir",
        "optdir"
    ],
    "workdir": {
        "container": "/home/kfarrow/work"
    },
    "optdir": {
        "host": "/Users/kfarrow/opt/dax-opt",
        "container": "/home/kfarrow/opt"
    },
    "awsdir": {
        "host": "/Users/kfarrow/.aws",
        "container": "/home/kfarrow/.aws"
    },
  
    "sshdir": {
        "host": "/Users/kfarrow/.ssh",
        "container": "/home/kfarrow/.ssh"
    },
  
    "gpgdir": {
        "host": "/Users/kfarrow/.gnupg",
        "container": "/home/kfarrow/.gnupg"
    },
    "msfdir": {
        "host": "/Users/kfarrow/.msf4",
        "container": "/home/kfarrow/.msf4"
    }
}

 
